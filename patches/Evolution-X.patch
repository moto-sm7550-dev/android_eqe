Subject: [PATCH] eqe: Bringup! Evolution-X

---
 lineage_eqe.mk                                           | 3 +++
 .../lineage-sdk/lineage/res/res/values/config.xml        | 2 +-
 .../apps/Updater/app/src/main/res/values/strings.xml     | 9 +++++++++
 3 files changed, 13 insertions(+), 1 deletion(-)
 create mode 100644 overlay-lineage/packages/apps/Updater/app/src/main/res/values/strings.xml

diff --git a/lineage_eqe.mk b/lineage_eqe.mk
index 9d9a619..02c75bd 100644
--- a/lineage_eqe.mk
+++ b/lineage_eqe.mk
@@ -15,6 +15,9 @@ $(call inherit-product, device/motorola/eqe/device.mk)
 # Inherit some common Lineage stuff.
 $(call inherit-product, vendor/lineage/config/common_full_phone.mk)
 
+TARGET_HAS_UDFPS := true
+TARGET_INCLUDE_ACCORD := false
+
 PRODUCT_NAME := lineage_eqe
 PRODUCT_DEVICE := eqe
 PRODUCT_MANUFACTURER := motorola
diff --git a/overlay-lineage/lineage-sdk/lineage/res/res/values/config.xml b/overlay-lineage/lineage-sdk/lineage/res/res/values/config.xml
index 0727b27..302d45d 100644
--- a/overlay-lineage/lineage-sdk/lineage/res/res/values/config.xml
+++ b/overlay-lineage/lineage-sdk/lineage/res/res/values/config.xml
@@ -11,9 +11,9 @@
     <!-- Defines the actions shown in advanced reboot submenu -->
     <string-array name="config_restartActionsList">
         <item>restart</item>
+        <item>restart_systemui</item>
         <item>restart_recovery</item>
         <item>restart_bootloader</item>
-        <item>restart_fastboot</item>
      </string-array>
 
      <!-- Whether the device supports wireless charging or not -->
diff --git a/overlay-lineage/packages/apps/Updater/app/src/main/res/values/strings.xml b/overlay-lineage/packages/apps/Updater/app/src/main/res/values/strings.xml
new file mode 100644
index 0000000..e0d26a5
--- /dev/null
+++ b/overlay-lineage/packages/apps/Updater/app/src/main/res/values/strings.xml
@@ -0,0 +1,9 @@
